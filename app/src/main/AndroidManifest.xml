<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.doanltdd"
    android:installLocation="preferExternal">


    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".YoutobeActivity"
            android:theme="@style/AppThemeKenh"/>
        <activity
            android:name=".TuDaTraActivity"
            android:theme="@style/AppThemeTuDaTra" />
        <activity android:name=".TuCuaBanActivity"
            android:theme="@style/AppThemeTuCuaBan"/>
        <activity android:name=".DichVanBanActivity" />
        <activity android:name=".welcomeActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WordMeaningActivity" />
        <activity android:name=".SettingActivity" />
    </application>

</manifest>